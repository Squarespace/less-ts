{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es6"],
    "declaration": true,
    "downlevelIteration": false,
    "removeComments": false,
    "sourceMap": true,
    "inlineSources": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": false,
    "baseUrl": ".",
    "paths": {
			"@squarespace/*": ["packages/*/"]
    }
  }
}
